generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model user {
  id     Int         @id @default(autoincrement())
  name   String      @db.VarChar(99)
  email  String      @db.VarChar(99)
  status user_status
}

model note {
  id      Int    @id @default(autoincrement())
  message String @db.VarChar(99)
}

enum user_status {
  active
  inactive
}
